{
    "name": "oneseven9955/logkeeper",
    "description": "LogKeeper archives old files based on a customizable time delta, ensuring your logs remain organized, and easily manageable.",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "OneSeven9955\\LogKeeper\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OneSeven9955\\LogKeeper\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.1",
        "ext-zip": "*",
        "psr/log": "^3.0"
    },
    "require-dev": {
        "symfony/var-dumper": "^6.0",
        "phpunit/phpunit": "^10.5",
        "symfony/finder": "^6.4"
    }
}
